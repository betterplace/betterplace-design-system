import { Meta, StoryObj } from '@storybook/react'
<% if (figma) { %>
import FigmaCfg from './figma.lock'
<% } %>
import <%=compName%> from './<%=compFileName%>'

// More on default export: https://storybook.js.org/docs/react/writing-stories/introduction#default-export
export default {
  component: <%=compName%>,
  // More on argTypes: https://storybook.js.org/docs/react/api/argtypes
  // argTypes: {},
  <% if (figma) { %>
  parameters: {
    design: FigmaCfg.themes.length ? FigmaCfg.themes.map(({url, name}) => ({url, name, type: 'figspec', accessToken: import.meta.env.STORYBOOK_FIGMA_ACCESS_TOKEN,})) : { type: 'figspec', accessToken: import.meta.env.STORYBOOK_FIGMA_ACCESS_TOKEN, url: FigmaCfg.url,},
  },
  <% } %>
} as Meta<typeof <%=compName%>>

<% if (!variants?.length) { %>
export const Default: StoryObj = {}
<%} else { variants.forEach(function(variant) {%>
export const <%=variant.name%> = {
  args: {
    <% variant.values.forEach(function(value) { %> <%-value.name%>: <%-value.value%>, <% }) %>
  }
}
<% })} %>
